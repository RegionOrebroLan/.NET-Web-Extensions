@model RecaptchaForm
@if(Model != null)
{
	<form asp-action="Post" asp-controller="Recaptcha" asp-fragment="example-form" data-recaptcha-enabled="true">
		<fieldset>
			<div class="mb-3">
				<label asp-for="Value"></label>
				<input asp-for="Value" class="form-control" />
			</div>
			<button class="btn btn-primary" type="submit">Post</button>
		</fieldset>
	</form>
}